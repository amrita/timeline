<h1>New eventdata</h1>

<% form_for(@eventdata) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :dataid %><br />
    <%= f.text_field :dataid %>
  </p>
  <p>
    <%= f.label :eventid %><br />
    <%= f.text_field :eventid %>
  </p>
  <p>
    <%= f.label :tag %><br />
    <%= f.text_field :tag %>
  </p>
  <p>
    <%= f.label :tagdesc %><br />
    <%= f.text_field :tagdesc %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', eventdatas_path %>