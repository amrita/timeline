<h1>Listing eventdatas</h1>

<table>
  <tr>
    <th>Dataid</th>
    <th>Eventid</th>
    <th>Tag</th>
    <th>Tagdesc</th>
  </tr>

<% @eventdatas.each do |eventdata| %>
  <tr>
    <td><%=h eventdata.dataid %></td>
    <td><%=h eventdata.eventid %></td>
    <td><%=h eventdata.tag %></td>
    <td><%=h eventdata.tagdesc %></td>
    <td><%= link_to 'Show', eventdata %></td>
    <td><%= link_to 'Edit', edit_eventdata_path(eventdata) %></td>
    <td><%= link_to 'Destroy', eventdata, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New eventdata', new_eventdata_path %>